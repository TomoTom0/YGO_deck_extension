{
  "name": "遊戯王デッキインポート/エクスポート",
  "description": "公式カードデータベースのデッキを.ydkファイルとしてエクスポート、および.ydkファイルをインポートすることができます。",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["https://www.db.yugioh-card.com/yugiohdb/*"],
    "js": ["jquery-3.5.1.min.js", "ChEx.js","async.min.js", "content_script.js"]
  }],
  "web_accessible_resources":["data/ygo_db_simple.csv"],
  "options_page": "options.html",
  "permissions": [
    "https://www.db.yugioh-card.com/yugiohdb/*",
    "storage"
  ]
}
